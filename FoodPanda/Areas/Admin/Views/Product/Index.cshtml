

<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Product List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Product" asp-action="Upsert" class="btn btn-primary">
                + Add New Food Item
            </a>
        </div>
    </div>
    <br/>
    <br/>
    <table class="table table-bordered table-striped" id="tblData" style="width:100%">
        <thead>
            <tr>
                <th>Food Item </th>
                <th>Description</th>
                @*<th>imageUrl</th>*@
                <th>Price</th>
                <th>Restaurant</th>
                <th>Food Type</th>
                <th>Rating</th>
                <th>Address</th>
                <th>Food Catagory</th>
                <th>Action</th>


            </tr>
        </thead>
       
    </table>
</div>
@section Scripts{
    <script src="~/js/product.js"></script>
}