

<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Product List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Product" asp-action="Upsert" class="btn btn-primary">
                + Add New Food Item
            </a>
        </div>
    </div>
    <br/>
    <br/>
    <table class="table table-bordered table-striped" id="tblData" style="width:100%">
        <thead>
            <tr>
                <th>productName</th>
                <th>description</th>
                <th>imageUrl</th>
                <th>price</th>
                <th>restaurantName</th>
                <th>restaurantImage</th>
                <th>restaurantRating</th>
                <th>address</th>
                <th>foodCatagoryType</th>
                <th>Action</th>


            </tr>
        </thead>
       
    </table>
</div>
