@model FoodCatagory
@{
    ViewData["Title"] = "Create";
}

<div class="container px-5">
    <form method="post" asp-action="Create">
        <div class="mb-3">
            <label class="form-label" asp-for="FoodCatagoryType">FoodCatagory Name</label>
            <input asp-for="FoodCatagoryType" class="form-control" id="FoodCatagoryName" type="text" />
            <span class="text-danger" asp-validation-for="FoodCatagoryType"></span>
        </div>
      
      
        <div class="d-flex">
            <button class="btn btn-primary btn-lg mr-4" type="submit">Submit</button>

            <a asp-controller="FoodCatagory" asp-action="Index" class="btn btn-danger btn-lg ">Cancle</a>
        </div>
    </form>
</div>
@*<script src="https://cdn.startbootstrap.com/sb-forms-latest.js"></script>*@
@section Scripts
{
    @{
    <partial name="_ValidationScriptsPartial" />
    }
}
