@model FoodCatagory
@{
    ViewData["Title"] = "Edit FoodCatagory";
}

<div class="container px-5">
    <form id="contactForm" data-sb-form-api-token="API_TOKEN" method="post" asp-action="Edit">
        <input asp-for="FoodCatagoryId" hidden />
        <div class="mb-3">
            <label class="form-label" asp-for="FoodCatagoryType">Restaurant Name</label>
            <input asp-for="FoodCatagoryType" class="form-control" id="restaurantName" type="text" data-sb-validations="required" />
            <span class="text-danger" asp-validation-for="FoodCatagoryType"></span>
            
        </div>
     
        <div class="d-flex">
            <button class="btn btn-primary btn-lg mr-4" id="submitButton" type="submit">Update</button>

        </div>

       
       
    </form>
</div>
<script src="https://cdn.startbootstrap.com/sb-forms-latest.js"></script>
@section Scripts
{
    @{
    <partial name="_ValidationScriptsPartial" />
    }
}
